{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0, s-maxage=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Surrogate-Control",
          "value": "no-store"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-store"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Vercel-Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Vercel-Cache",
          "value": "MISS"
        },
        {
          "key": "X-Accel-Expires",
          "value": "0"
        },
        {
          "key": "X-Proxy-Cache",
          "value": "BYPASS"
        },
        {
          "key": "X-SW-Cache",
          "value": "no-cache"
        }
      ]
    }
  ]
} 